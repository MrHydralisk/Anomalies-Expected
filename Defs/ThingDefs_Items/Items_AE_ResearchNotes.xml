<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef Name="AEResearchNotesBase" ParentName="ResourceBase" Abstract="True">
		<description>A research notes related to anomaly. It can be studied to help unlock new anomaly research.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AEResearchNotes</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(0.75, 0.75)</drawSize>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<stackLimit>10</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>500</MarketValue>
			<Mass>1</Mass>
			<Flammability>0</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
		</statBases>
		<intricate>true</intricate>
		<thingCategories>
			<li>AnomalyStudyNotes</li>
		</thingCategories>
		<isTechHediff>true</isTechHediff>
	</ThingDef>
	
	

	<ThingDef ParentName="AEResearchNotesBase">
		<defName>AE_ResearchNotesABA</defName>
		<label>research note #AB01</label>
		<comps>
			<li Class="CompProperties_CompAnalyzableUnlockResearch">
				<analysisID>1300101</analysisID>

				<!-- Analyzable -->

				<analysisDurationHours>4</analysisDurationHours>
				<destroyedOnAnalyzed>true</destroyedOnAnalyzed>
				<completedLetterLabel>Neural Collective Processing Unit research note studied: {RESEARCH} unlocked</completedLetterLabel>
				<completedLetter>By studying research note, {PAWN_labelShort} has acquired the insight needed for the research project(s) {RESEARCH}.\n\nYour researchers can now use research benches to begin researching how to actually use the technology.</completedLetter>
				<completedLetterDef>PositiveEvent</completedLetterDef>

				<!-- Interactable -->

				<activateTexPath>UI/Icons/Study</activateTexPath>

				<activateLabelString>Analyze...</activateLabelString>
				<activateDescString>Analyze the research note.</activateDescString>
				<jobString>Analyze research note</jobString>
				<guiLabelString>Choose who should analyze this</guiLabelString>
				<inspectString>Can be analyzed by a colonist.</inspectString>

				<targetingParameters>
					<canTargetBuildings>false</canTargetBuildings>
					<canTargetAnimals>false</canTargetAnimals>
					<canTargetMechs>false</canTargetMechs>
					<onlyTargetControlledPawns>true</onlyTargetControlledPawns>
				</targetingParameters>
			</li>
		</comps>
	</ThingDef>
	
	

