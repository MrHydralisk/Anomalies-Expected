<Defs>

	<RecipeDef ParentName="SurgeryInstallImplantBase">
		<defName>InstallAEForbiddenFruitImplant</defName>
		<label>install implant of forbidden knowledge</label>
		<description>Install an implant of forbidden knowledge.</description>
		<descriptionHyperlinks>
			<ThingDef>AEForbiddenFruitImplant</ThingDef>
			<HediffDef>AEForbiddenFruitImplant</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing implant of forbidden knowledge.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>AEForbiddenFruitImplant</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>AEForbiddenFruitImplant</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Brain</li>
		</appliedOnFixedBodyParts>
		<addsHediff>AEForbiddenFruitImplant</addsHediff>
		<surgerySuccessChanceFactor>99999</surgerySuccessChanceFactor>
	</RecipeDef>

	<RecipeDef ParentName="SurgeryRemoveImplantBase">
		<defName>RemoveAEForbiddenFruitImplant</defName>
		<label>remove implant of forbidden knowledge</label>
		<description>Remove implant of forbidden knowledge.</description>
		<descriptionHyperlinks>
			<ThingDef>AEForbiddenFruitImplant</ThingDef>
			<HediffDef>AEForbiddenFruitImplant</HediffDef>
		</descriptionHyperlinks>
		<jobString>Removing implant of forbidden knowledge.</jobString>
		<removesHediff>AEForbiddenFruitImplant</removesHediff>
	</RecipeDef>
	


